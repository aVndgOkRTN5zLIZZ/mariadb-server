set SQL_MODE= oracle;

select t1.a, t4.d, t2.b, t3.c
  from t1, t2, t3, t4
  where
    t1.a = t2.b(+) and
    t1.a = t3.c(+) and
    t4.d = t2.b(+) and
    t4.d = t3.c(+);

explain extended
select t1.a, t4.d, t2.b, t3.c
  from t1, t2, t3, t4
  where
    t1.a = t2.b(+) and
    t1.a = t3.c(+) and
    t4.d = t2.b(+) and
    t4.d = t3.c(+);

